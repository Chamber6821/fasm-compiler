{
  "exclude": [
    "./dist/**/*"
  ],
  "compilerOptions": {
    "target": "ES5",
    "module": "CommonJS",
    "lib": ["ES6"],
    "outDir": "./dist",
    "declaration": true,
    "rootDir": "./src",
    "baseUrl": "./src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "downlevelIteration": true
  }
}
